<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RatGame.LevelSelect">
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="LEVELS" BorderPane.alignment="CENTER">
         <font>
            <Font size="44.0" />
         </font>
      </Text>
   </top>
   <center>
      <VBox fx:id="LevelButtons" alignment="CENTER" prefHeight="500.0" prefWidth="1000.0" spacing="10.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <Button fx:id="back" mnemonicParsing="false" onAction="#switchToMainMenu" text="Back" BorderPane.alignment="TOP_CENTER" />
   </bottom>
</BorderPane>
